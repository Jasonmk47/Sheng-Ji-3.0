{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsrules": {
    "ordered-imports" : false,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rules": {
    "ordered-imports" : false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false
  },
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "**/*.d.ts"
    ]
  }
}
